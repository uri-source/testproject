<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
            <label>Service 360 Dashboard Access</label>
            <description>Provides access to Service 360 Dashboard and health score fields for service managers and representatives</description><!-- Field Permissions - Make custom fields visible and editable based on isEditable (required fields are excluded) -->
            
            <fieldPermissions>
                
                <editable>true</editable>
                
                <field>Account.SER_HealthScore__c</field>
                <readable>true</readable>
            </fieldPermissions>
            
            
            <!-- Tab Settings - Make custom tabs visible -->
            
            <tabSettings>
                <tab>ServiceDashboardTab</tab>
                <visibility>Visible</visibility>
            </tabSettings>
            
            
            <!-- Application Visibility - Make custom applications available -->
            
            <applicationVisibilities>
                <application>Service360App</application>
                <visible>true</visible>
            </applicationVisibilities>
            
            
            <hasActivationRequired>false</hasActivationRequired>
        </PermissionSet>